package mathprg;
import  java.util.Scanner;
public class tfld {
        public static void main(String[] args) {
            Scanner scan=new Scanner(System.in);
            int n=scan.nextInt();
            while(n>0){
                int ld=n%10;
                System.out.print(ld);
                n=n/10;
            }
        }
    }
